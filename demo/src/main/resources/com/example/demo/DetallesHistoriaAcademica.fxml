<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo.DetallesHistoriaAcademicaController">
   <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" spacing="30.0">
      <children>
         <Label text="Historia Académica"/>
         <HBox alignment="CENTER">
            <children>
               <Label prefWidth="100.0" text="Estudiante:"/>
               <ComboBox fx:id="cbEstudiante" prefWidth="150.0"/>
            </children>
         </HBox>
         <HBox alignment="CENTER">
            <children>
               <Label prefWidth="100.0" text="Colegio Anterior:"/>
               <TextField fx:id="tfColegioAnterior" prefWidth="150.0"/>
            </children>
         </HBox>
         <HBox alignment="CENTER">
            <children>
               <Label prefWidth="100.0" text="Año:"/>
               <TextField fx:id="tfAño" prefWidth="150.0"/>
            </children>
         </HBox>
         <HBox alignment="CENTER">
            <children>
               <Label prefWidth="100.0" text="Grado:"/>
               <TextField fx:id="tfGrado" prefWidth="150.0"/>
            </children>
         </HBox>
         <HBox alignment="CENTER" spacing="30.0">
            <children>
               <Button fx:id="btnAceptar" mnemonicParsing="false" onAction="#clicAceptar" text="Aceptar"/>
               <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#clicCancelar" text="Cancelar"/>
            </children>
         </HBox>
      </children>
   </VBox>
</AnchorPane>
